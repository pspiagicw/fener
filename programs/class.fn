;; You can only have class methods, no attributes.
;; Meaning, you can't do this

;; var.name = "something else"

;; Declare a setter or some other method for that


class something
    fn init(name, surname)
        this.name = name
        this.surname = surname
    end
    fn hello()
        print("Hello ", this.name)
    end
    fn setName(name)
        this.name = name
    end
end

v = something("Chris", "Pratt")
v.hello()
v.setName("Trevor")
v.hello()

